{
  "name": "react-panel-split",
  "version": "0.0.1",
  "description": "A react split panel component based on antd",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.config.dev.js",
    "build": "npm run clean && npm run build:umd && npm run build:esm",
    "build:umd": "webpack",
    "build:esm": "tsc -p tsconfig.json --target ES5 --module ESNext --outDir esm",
    "clean": "rimraf dist esm",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix"
  },
  "keywords": [
    "React",
    "Split",
    "Panel",
    "Antd"
  ],
  "author": "Aaron",
  "license": "ISC",
  "devDependencies": {
    "@ant-design/icons": "^5.0.1",
    "@types/react-dom": "^18.0.11",
    "@types/react": "^18.0.38",
    "antd": "^5.4.5",
    "css-loader": "^6.7.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint": "^8.39.0",
    "html-webpack-plugin": "^5.5.1",
    "less-loader": "^11.1.0",
    "less": "^4.1.3",
    "react-dom": "^18.2.0",
    "react": "^18.2.0",
    "rimraf": "^5.0.1",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3",
    "webpack": "^5.80.0"
  },
  "peerDependencies": {
    "antd": ">=4.0.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
}
